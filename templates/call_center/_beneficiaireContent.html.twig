{% for r in result %}
    <tr>
        <td class="table-name">
        {% if is_granted('ROLE_CALL_CENTER') %}
            <a href="{{ path('call_center_Rdv',{ref:idRef,id:r.id}) }}">
                {% if r.personneMorale == 1 %}
                    {{ r.nomDuSiteBeneficiaireOperation }}
                {% else %}
                    {{ r.name }} {{ r.prenom }}
                {% endif %}</a>
        {% else %}
                <a href="{{ path('navigation_show',{id:r.id}) }}">  {% if r.personneMorale == 1 %}
                        {{ r.nomDuSiteBeneficiaireOperation }}
                    {% else %}
                        {{ r.name }} {{ r.prenom }}
                    {% endif %}</a>
        {% endif %}
        </td>
        <td class="table-name">{% if r.codePostal|length <=4 %}0{% endif %}{{ r.codePostal }}</td>
        <td class="table-name">{{ r.ville }}</td>
        {% if (r.statut== 0) %}
            <td class="table-name">
                <p>En attente</p>
            </td>
        {% else %}
            {% if date('now')> date(r.rdv) %}
                <td class="table-name">
                    <p>Contrôlé</p>
                </td>
            {% else %}
                <td class="table-name">
                    <p>En cours</p>
                </td>
            {% endif %}
        {% endif %}
    </tr>
{% endfor %}




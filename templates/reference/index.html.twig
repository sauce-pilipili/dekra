{% extends 'base.html.twig' %}
{% block title %}Références{% endblock %}
{% block body %}
    <div class="main">
        {{ include('include/_navigation.html.twig') }}
        <div class="content">
            <div class="container">
                <h4>Les references</h4>
                <div class="search-container">
                    <div class="search-input">
                        <form id="filters">
                            <label for="members-search"></label><input class="members-search" id="members-search" type="text"
                                                                       placeholder="Rechercher">
                        </form>
                    </div>
                </div>
                <div class="response-content" id="response-content">
                    {{ include('include/_referenceContent.html.twig') }}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/search.js') }}"></script>
{% endblock %}
